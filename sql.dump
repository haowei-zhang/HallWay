CREATE TABLE "recipe_step" (
    "id" integer NOT NULL PRIMARY KEY,
    "recipe_id" integer NOT NULL REFERENCES "recipe_recipe" ("id"),
    "step_num" integer unsigned NOT NULL,
    "description" varchar(1000) NOT NULL,
    "step_image" varchar(100),
    UNIQUE ("recipe_id", "step_num")
);
INSERT INTO "recipe_step" VALUES(2,3,1,'Cut the tomatoes in half or four, depending on their size. Turn the onion into wedges and thinly slice the garlic.','Recipe_photos/12c5f3f4-fe09-4b9e-b5e3-9d45552a088d.jpg');
INSERT INTO "recipe_step" VALUES(3,3,2,'Transfer your drumsticks to a large roasting tray.','Recipe_photos/2a090e13-8a94-4454-8ef7-9613efb4f469.jpg');
INSERT INTO "recipe_step" VALUES(4,3,3,'Season the chicken with a good pinch of lemon pepper.','Recipe_photos/2a038cee-0d2b-49b7-8801-8ccfa3856365.jpg');
INSERT INTO "recipe_step" VALUES(5,3,4,'Randomly place the garlic slices on the bottom of the tray.','Recipe_photos/237a33fe-4132-4a8d-b573-0d984b367b9a.jpg');
INSERT INTO "recipe_step" VALUES(6,3,5,'Add the tomatoes and onion wedges.','Recipe_photos/ea4a84e9-1c0b-42ea-bd5c-56f74d46e8d9.jpg');
INSERT INTO "recipe_step" VALUES(7,3,6,'I use a few pieces of tomato and onion to elevate the drumsticks so they won’t cook in their own juices.','Recipe_photos/d504e3c7-2b7d-478b-8838-58465b867bba.jpg');
INSERT INTO "recipe_step" VALUES(8,3,7,'Pour a little oil all over and season everything with kosher salt.','Recipe_photos/c22b7650-90a7-4cb7-8c99-e702d982dda2.jpg');
INSERT INTO "recipe_step" VALUES(9,3,8,'Push the rosemary sprigs in there. We’ll remove them later—we only need the flavour.','Recipe_photos/c8d8b909-19ed-4909-a419-d002f43048c9.jpg');
INSERT INTO "recipe_step" VALUES(10,3,9,'Strip the thyme sprigs and sprinkle the leaves all over.','Recipe_photos/843f07dc-f66d-4b17-8f98-d39c73f2ff98.jpg');
INSERT INTO "recipe_step" VALUES(11,3,10,'Squeeze the lemon juice over the chicken and pour in the chicken broth.','Recipe_photos/c989453a-a4d5-4228-8042-196a1a88a893.jpg');
INSERT INTO "recipe_step" VALUES(12,3,11,'So pretty, so flavourful, yet so simple. Finely mince the parsley while the chicken is roasting.','Recipe_photos/051683bf-9ae7-48b6-93a1-995a0a700465.jpg');
INSERT INTO "recipe_step" VALUES(13,3,12,'Roast the drumsticks at 200°C (400°F) for 40 minutes and give them an extra 3 to 5 minutes under a broiler. Discard the charred rosemary and sprinkle the parsley all over right before serving.','Recipe_photos/85f21bfa-7e03-4f7a-ab79-26fa68ba8190.jpg');
INSERT INTO "recipe_step" VALUES(14,4,1,'Cube the beef. I also use a beef shin. You can replace it with a soup bone. I never make soup without some sort of soup bone.','Recipe_photos/60e13b93-733a-4d30-8d2b-c3c3fa1cb023.jpg');
INSERT INTO "recipe_step" VALUES(15,4,2,'Mince the onions, bell pepper and peel and cube the potatoes. Grate the garlic.','Recipe_photos/6c5a3bcc-21e8-456c-890a-90ceef0caa21.jpg');
INSERT INTO "recipe_step" VALUES(16,4,3,'Heat a few drops of oil and lightly sauté the onions and pepper. Nothing major, just a minute or 2. Add the garlic and give it another minute.','Recipe_photos/f1145c9d-90d5-40d6-9dac-3425b6873816.jpg');
INSERT INTO "recipe_step" VALUES(17,4,4,'Stir in the tomato paste and give it some time to sweeten up.','Recipe_photos/ff4d8fd5-a8ca-49c5-80c3-da65b90130b3.jpg');
INSERT INTO "recipe_step" VALUES(18,4,5,'In with the caraway seeds, thyme, dried chili flakes and sweet paprika powder.','Recipe_photos/6f02bfee-62ca-489b-b246-1e445141a212.jpg');
INSERT INTO "recipe_step" VALUES(19,4,6,'I used the sweet Hungarian paprika powder for this. It’s also available in a hot powdered version. Just look for sweet paprika powder, not the regular kind.','Recipe_photos/5ca5c97c-aec7-48a7-b757-08693f0dd093.jpg');
INSERT INTO "recipe_step" VALUES(20,4,7,'Check out the water shot! Add the water and bouillon cubes.','Recipe_photos/21617bf7-7066-4b32-8cab-1be223ada015.jpg');
INSERT INTO "recipe_step" VALUES(21,4,8,'Add the meat. And again, the bone/marrow adds so much flavor and richness to the soup.','Recipe_photos/d03b144d-8bb2-483a-a492-50c174684550.jpg');
INSERT INTO "recipe_step" VALUES(22,4,9,'Add a pinch of black pepper, the vinegar and stir in the potatoes.','Recipe_photos/defd8a22-d9eb-4a26-bb12-7965d3b42cd0.jpg');
INSERT INTO "recipe_step" VALUES(23,4,10,'I added my potatoes at the start so that they’d slowly cook slightly apart and thicken the soup. You can also add them later.','Recipe_photos/4f5136a2-0630-42b7-a2f3-8b34aa4fe115.jpg');
INSERT INTO "recipe_step" VALUES(24,4,11,'And the bay leaves, bring the soup to a boil and simmer it over low heat for 2 and a half hours.','Recipe_photos/52ec08de-5c0d-4499-af80-40e7b5d63128.jpg');
INSERT INTO "recipe_step" VALUES(25,4,12,'It smelled SO fabulous.','Recipe_photos/ecb9b3bb-6d45-4b52-8bca-70359cfb1676.jpg');
INSERT INTO "recipe_step" VALUES(26,4,13,'Remove the beef shin and bone, pick the meat apart and put it back in with the soup.','Recipe_photos/164caebc-0f82-4a7a-a3c6-b1e2d49005bc.jpg');
INSERT INTO "recipe_step" VALUES(27,4,14,'Discard the bay leaves and season the soup with salt and pepper to taste. Garnish the soup with parsley and a scoop of sour cream, if you like.','Recipe_photos/006f7e2f-66b6-4079-be7e-b8903b98c7fc.jpg');
INSERT INTO "recipe_step" VALUES(28,5,1,'Combine the flour with the paprika powder, mustard powder, 1/2 tsp salt, pepper and nutmeg.','Recipe_photos/23be16da-2d0f-4896-9868-a10a97d16e2e.jpg');
INSERT INTO "recipe_step" VALUES(29,5,2,'Either mince the onions or, like me, turn them into quart-rings. Grate the garlic.','Recipe_photos/bc400371-f240-4f87-bae3-e711cba1dedd.jpg');
INSERT INTO "recipe_step" VALUES(30,5,3,'Cut the beef in chunks and don’t make them too small. Sprinkle the seasoned flour mix all over.','Recipe_photos/4b2c516d-76cd-4aa2-b05d-3077d0bbdc45.jpg');
INSERT INTO "recipe_step" VALUES(31,5,4,'Heat some butter or oil, whatever you prefer, and quickly brown the beef cubes. Transfer them to a plate and cover with aluminum foil to keep them warm.','Recipe_photos/93cbb367-fee7-4d29-83c8-bc62af1bc33a.jpg');
INSERT INTO "recipe_step" VALUES(32,5,5,'Don’t overcrowd the pan, brown the beef cubes in small batches.','Recipe_photos/398b0ffa-1195-4798-852d-a0385a95542e.jpg');
INSERT INTO "recipe_step" VALUES(33,5,6,'Sauté the onion for 4 minutes. Add the garlic and give it another minute.','Recipe_photos/efb33b93-d05d-4869-90b7-c5fab065851e.jpg');
INSERT INTO "recipe_step" VALUES(34,5,7,'Put the meat back in with the onions and pour in the beef broth.','Recipe_photos/b1cec5a5-d107-4f09-a163-d1140eac63ac.jpg');
INSERT INTO "recipe_step" VALUES(35,5,8,'Add the bay leaves. Optionally you can add one or two beef herb envelopes (Maggi rundvlees kruidenbuiltjes).','Recipe_photos/2dc6614b-307a-480f-bed1-d2e4a5d4eb2f.jpg');
INSERT INTO "recipe_step" VALUES(36,5,9,'Pop the lid on, bring it up to a boil and simmer the beef for 2 to 2 and a half hours','Recipe_photos/ac396b08-e131-4198-89d4-ae05b8c3a9b9.jpg');
INSERT INTO "recipe_step" VALUES(37,5,10,'After almost 2 hours you clean up the sprouts and peel the carrot, parsnip, potatoes and pumpkin. Chop it all in equal bit-sized pieces.','Recipe_photos/026d69f2-d19d-4ac5-87b5-eacc114c35c5.jpg');
INSERT INTO "recipe_step" VALUES(38,5,11,'After two and a half hours my beef nearly melted on my tongue. Heavenly.','Recipe_photos/a2051a71-5262-4ace-abc0-153661ccd282.jpg');
INSERT INTO "recipe_step" VALUES(39,5,12,'In with the vegetables. Mix it all up.','Recipe_photos/1ed97147-356c-42be-8b37-cafc5f29d76a.jpg');
INSERT INTO "recipe_step" VALUES(40,5,13,'Season with a pinch of salt (be moderate, you can add more later), dried thyme and pepper.','Recipe_photos/558c7f0d-2d5d-42d6-8576-76a3064fd515.jpg');
INSERT INTO "recipe_step" VALUES(41,5,14,'Bring it back to a boil and simmer the stew for about 30 minutes, until the vegetables are tender. Stir now and then.','Recipe_photos/29a8f117-fcb5-4587-aea2-f3930320a3b0.jpg');
INSERT INTO "recipe_step" VALUES(42,5,15,'Discard the bay leaves and taste to check the seasoning. Adjust when needed.','Recipe_photos/a63a776e-7b4a-4ad4-8cac-1f25021ecfdb.jpg');
INSERT INTO "recipe_step" VALUES(43,6,1,'Killer combo: garlic, rosemary and parsley.','Recipe_photos/61c97241-fc4b-43f3-b1dd-f562d3c2e5c7.jpg');
INSERT INTO "recipe_step" VALUES(44,6,2,'You can finely mince all the ingredients, or use the blender/food processor. I did.','Recipe_photos/020d55b9-3c6b-4ed3-a7e1-c671bd6c2d63.jpg');
INSERT INTO "recipe_step" VALUES(45,6,3,'Brush a little flavored oil all over the chicken.','Recipe_photos/bd29859e-238e-46b8-953c-2d1a8acbfdde.jpg');
INSERT INTO "recipe_step" VALUES(46,6,4,'Trim the crusts and cube the bread. Transfer it to a casserole.','Recipe_photos/441788e1-2662-4f76-8ee9-3433dd3d99db.jpg');
INSERT INTO "recipe_step" VALUES(47,6,5,'Mix the bread with the remaining herbed oil and sprinkle a bit of salt over the bread.','Recipe_photos/cdc1ac30-86b9-4cad-9153-3a74c6f4ac2d.jpg');
INSERT INTO "recipe_step" VALUES(48,6,6,'Heat your stove top grill and grill the chicken breast roughly 6 to 8 minutes on each side.','Recipe_photos/19a8759f-2552-4731-ab97-8ca0caf81338.jpg');
INSERT INTO "recipe_step" VALUES(49,6,7,'The croutons should be done by now. And the oven should still be warm. Perfect.','Recipe_photos/58b47cea-2c71-4e4c-9d56-6df4f94241ba.jpg');
INSERT INTO "recipe_step" VALUES(50,6,8,'Normally I would have pounded the chicken to cut cooking time, but for a salad I want to keep the chicken as is. So I give it 10 minutes in a hot oven.','Recipe_photos/a7326f17-8492-46bc-b20c-5dca555d0cf3.jpg');
INSERT INTO "recipe_step" VALUES(51,6,9,'Mix all ingredients listed under dressing and grate the garlic in there. Stir. It’ll be grainy at first.','Recipe_photos/8ad7b198-49d2-47eb-9dbe-d75b0def4b9e.jpg');
INSERT INTO "recipe_step" VALUES(52,6,10,'Soon it will turn into a creamy dressing.','Recipe_photos/4bbc2472-af05-4126-a727-16cc5a2f8304.jpg');
INSERT INTO "recipe_step" VALUES(53,6,11,'Wash and dry the romaine lettuce.','Recipe_photos/eb359864-3cec-4c90-aefb-3bd8cef8c974.jpg');
INSERT INTO "recipe_step" VALUES(54,6,12,'You can cut out the ribs (they can be tough), but I slice it in 2 inch pieces and rip the leaves off the ribs.','Recipe_photos/0d973a09-fe8d-44a8-b1e7-16dd3a79bf17.jpg');
INSERT INTO "recipe_step" VALUES(55,6,13,'Thinly slice the chicken breast. Grate the Parmesan or use a vegetable peeler to shave curls.','Recipe_photos/bf332357-eaa2-4038-8c35-cd9612fb7471.jpg');
INSERT INTO "recipe_step" VALUES(56,6,14,'This truly is the king of all salads.','Recipe_photos/633a1a9a-11ec-46ae-a4ad-3077e739c843.jpg');
INSERT INTO "recipe_step" VALUES(57,7,1,'Chop the onion. This is about as hard as the recipe gets.','Recipe_photos/f8a2a1cf-6133-40b5-b694-b4cf3e48f082.jpg');
INSERT INTO "recipe_step" VALUES(58,7,2,'Heat a tbsp oil and sauté the onion for 4 to 5 minutes until translucent. Don’t let the onion brown!','Recipe_photos/7d4ee8da-78c6-415c-a392-0a2852653691.jpg');
INSERT INTO "recipe_step" VALUES(59,7,3,'Add the tomato paste and cook for a minute to sweeten it up.','Recipe_photos/49ef4422-4017-4f37-bb16-915d00a39ea6.jpg');
INSERT INTO "recipe_step" VALUES(60,7,4,'Pour in the cans of diced tomatoes. Fill one of the cans with water and pour it in as well.','Recipe_photos/7e3e4102-256f-4d72-897a-1777c897e430.jpg');
INSERT INTO "recipe_step" VALUES(61,7,5,'Season with the bouillon cube, sugar, Italians herbs and pepper. The sugar is merely used to neutralize the acidity.','Recipe_photos/983b78a8-38dc-4e1e-a2aa-4e8a882eeb28.jpg');
INSERT INTO "recipe_step" VALUES(62,7,6,'Bring to a boil, put the lid on and simmer for 15 minutes.','Recipe_photos/4d4ffb4a-0aea-435a-a775-e2d5ef3603af.jpg');
INSERT INTO "recipe_step" VALUES(63,7,7,'Use an immersion blender or a regular blender to smoothen the soup.','Recipe_photos/a8be6ac4-57eb-4cfe-b5c5-0819b5260ed0.jpg');
INSERT INTO "recipe_step" VALUES(64,7,8,'Pour the soup back into the pan.','Recipe_photos/615f5a5c-e4c4-4449-b283-e61c3748c453.jpg');
INSERT INTO "recipe_step" VALUES(65,7,9,'Melt the Boursin into the soup.','Recipe_photos/11b48883-05f2-4c29-bd7a-b040159731cc.jpg');
INSERT INTO "recipe_step" VALUES(66,7,10,'Stir in a full tbsp green pesto. This will give a subtle flavor. Want a more intense flavor, just add more.','Recipe_photos/fdad7b3c-0c44-41bd-a153-ba264e0f4160.jpg');
INSERT INTO "recipe_step" VALUES(67,7,11,'Add salt to taste (for me 2/3 tsp was enough) and stir in 2 to 3 tbsp cream as a finishing touch.','Recipe_photos/c9219f68-d783-4de2-a1e8-e6b14d5d03d7.jpg');
INSERT INTO "recipe_step" VALUES(68,7,12,'Garnish with fresh basil and serve with lots of crusty bread. Simple can be so delicious.','Recipe_photos/ac48ac6f-d52f-4209-bd12-d9e21500778b.jpg');
INSERT INTO "recipe_step" VALUES(69,8,1,'Chop, chop. Thinly slice the red onion, tomatoes, avocado and mozzarella. Give the basil a chop as well.','Recipe_photos/d4fc4fda-6083-4837-97dd-dec72d5fff42.jpg');
INSERT INTO "recipe_step" VALUES(70,8,2,'I used a pre-baked focaccia. This one had garlic and rosemary and I think a hint of dried tomatoes in it. I’m going to grill it like this, straight out of the wrapping. It’ll cook on the grill. ','Recipe_photos/75c4b351-114e-4616-ae98-5c164e6abe49.jpg');
INSERT INTO "recipe_step" VALUES(71,8,3,'Open it up and spread a thin layer of green pesto on both sides. I haven’t tried it with pesto rosso yet, but I can already tell that will be mind-blowing as well.','Recipe_photos/1a4c9049-ea1a-4311-8b98-757b20a70748.jpg');
INSERT INTO "recipe_step" VALUES(72,8,4,'Starting with the mozzarella. ','Recipe_photos/61b47050-1c9b-477f-85a5-d013a72bdb73.jpg');
INSERT INTO "recipe_step" VALUES(73,8,5,'Top the cheese with the tomato slices.','Recipe_photos/e420f6ff-2a76-4f69-828a-8d4460384cbe.jpg');
INSERT INTO "recipe_step" VALUES(74,8,6,'Season with salt, freshly cracked black pepper and basil','Recipe_photos/462d89d0-225f-42d3-9318-13059ea43af0.jpg');
INSERT INTO "recipe_step" VALUES(75,8,7,'Put the onions right on top of all that.','Recipe_photos/4fc0e1ab-533a-4591-9a4e-1c99c583936a.jpg');
INSERT INTO "recipe_step" VALUES(76,8,8,'And last but definitely not least, the avocado. This adds a wonderful creaminess to it all. ','Recipe_photos/ca04f514-e955-47fc-ba5b-10356bace9ac.jpg');
INSERT INTO "recipe_step" VALUES(77,8,9,'Place the other half of the focaccia on top, gently push it down and lightly brush (or spray) it with a little olive oil. ','Recipe_photos/4355ad43-a221-4265-a290-11f970682a0e.jpg');
INSERT INTO "recipe_step" VALUES(78,8,10,'Place the focaccia on the grill and leave it there for 3 minutes before rotating it 90 degrees to get those restaurant-style grill stripes and give it another 3 minutes. Then flip it over and do the same on the other side of the focaccia. This was the uncooked side still on top. Had a bit of a problem flipping it over with my camera dangling in the way. ','Recipe_photos/96a9b96d-244a-4fab-acaf-7c578794e9cf.jpg');
INSERT INTO "recipe_step" VALUES(79,8,11,'I sliced our focaccia in 3 pieces and served it with lots of fresh fruit and salad leaves. It was finger lickin’ good.','Recipe_photos/213d1715-c378-4aba-b55c-0f14d48716f0.jpg');
INSERT INTO "recipe_step" VALUES(80,9,1,'  In a bowl you combine the soy sauce, molasses, hoisin sauce, sesame oil and brown sugar.','Recipe_photos/9624c2bf-ebe0-4e17-aef8-1b9cf016adda.jpg');
INSERT INTO "recipe_step" VALUES(81,9,2,'Add the tabasco, pepper, chili powder and chili flakes. This will give a wonderful kick!','Recipe_photos/a6411e83-8d82-45da-ae17-f541d4c4cc19.jpg');
INSERT INTO "recipe_step" VALUES(82,9,3,'Grate the ginger and garlic in there and squeeze in the lemon juice. The scent is already killing at this point.','Recipe_photos/1b4a0259-a7ff-4a39-88c0-2e67a77c3936.jpg');
INSERT INTO "recipe_step" VALUES(83,9,4,'I finished the sauce with sweet chili sauce. So, so good.','Recipe_photos/cf2cb53a-26da-4b39-a86d-d310d21a56dc.jpg');
INSERT INTO "recipe_step" VALUES(84,9,5,'They don’t sell country-style ribs here, so I opted for meaty spare ribs. Matches my spare tire.','Recipe_photos/7a70ffaf-b04d-4a66-8725-effc4dd30cc6.jpg');
INSERT INTO "recipe_step" VALUES(85,9,6,'My butcher was sweet enough to remove that pesky membrane for me. Oh, how I love the man.','Recipe_photos/395c06f2-2132-41e9-bca3-daaad51005a6.jpg');
INSERT INTO "recipe_step" VALUES(86,9,7,'Pile them up in your slow cooker.','Recipe_photos/75e5899b-e7f2-4780-8be9-6bf2b3eabe6b.jpg');
INSERT INTO "recipe_step" VALUES(87,9,8,'Pour all the yummy sauce in there.','Recipe_photos/ec021105-1b0c-422e-9a89-add04f3fbe8d.jpg');
INSERT INTO "recipe_step" VALUES(88,9,9,'Dig your hands in there and make sure both sides of the ribs are coated.','Recipe_photos/64a54fbe-4e84-46bf-9a39-4823584c88c8.jpg');
INSERT INTO "recipe_step" VALUES(89,9,10,'The sauce will lurk at the bottom, so I brought the ones at the bottom on top a few times throughout the cooking process.','Recipe_photos/1b5c76c1-a82f-4592-a28b-51f4a35f669f.jpg');
INSERT INTO "recipe_step" VALUES(90,9,11,'That gave them all a chance to get a bit of color.','Recipe_photos/67dfab8b-47b5-49bb-b11d-b9298a17e446.jpg');
INSERT INTO "recipe_step" VALUES(91,9,12,'After 8 hours these will be incredibly tender and moist fall-off-the-bone kinda ribs. Delicious.','Recipe_photos/98059c12-51ae-4159-81da-c465f5a08703.jpg');
INSERT INTO "recipe_step" VALUES(92,9,13,'Preheat your broiler/grill, slap them on a grilling rack (6 inches from source of heat) and give them 10 to 15 minutes.','Recipe_photos/0e0d03bb-b85c-41ee-a841-5709c0b854a6.jpg');
INSERT INTO "recipe_step" VALUES(93,9,14,'Now you’re left with all these cooking juices. Terrible, isn’t it? Mix the cornstarch with some water and pour it in. Simmer the sauce for a few more minutes. Peppery Asian Ribs','Recipe_photos/0d7cfac0-c616-416f-a66e-889ed13c4bed.jpg');
INSERT INTO "recipe_step" VALUES(94,9,15,'Pour some of the sauce over the ribs and serve the rest on the side. Perfect match for this was steamed rice and sambal goreng buncis. ','Recipe_photos/01bc0d78-600c-425c-a934-08c2c3c1d029.jpg');
INSERT INTO "recipe_step" VALUES(95,10,1,'Perfect recipe to put these absolutely gorgeous Eden Classic Damast knives that I got from knivesandtools to the test!','Recipe_photos/23b95598-21da-4969-830a-ed7bb90eb0e2.jpg');
INSERT INTO "recipe_step" VALUES(96,10,2,'The stuff. Pretty basic, really.','Recipe_photos/6e34e974-f6c1-4122-b6e9-d0068e8bbe5f.jpg');
INSERT INTO "recipe_step" VALUES(97,10,3,'Finely mince the onion, garlic and de-seeded chill pepper. I’m mincing the garlic so it will have a mellower flavour.','Recipe_photos/db448709-f666-42a6-8808-87f336935851.jpg');
INSERT INTO "recipe_step" VALUES(98,10,4,'This Surinamese recipe calls for plaice fish—a European flatfish. You can replace it with sole fish or even haddock.','Recipe_photos/0d70f6d3-41c6-4546-913c-1bca0cd8c3e5.jpg');
INSERT INTO "recipe_step" VALUES(99,10,5,'Squeeze the lemon juice all over the fish and lightly season it with salt.','Recipe_photos/e61e42db-f82a-4fd4-afea-eb0f2fd3d685.jpg');
INSERT INTO "recipe_step" VALUES(100,10,6,'Roll up the plaice (or sole) and secure them with cocktail sticks.','Recipe_photos/d1a0609e-359a-462c-9ff0-fa99653a5870.jpg');
INSERT INTO "recipe_step" VALUES(101,10,7,'Heat the butter and oil and sauté the onion for a minute or 3 to 4, until translucent.','Recipe_photos/3a6ca77c-2a77-4a17-8de8-e2bb733daaab.jpg');
INSERT INTO "recipe_step" VALUES(102,10,8,'Add the garlic and cook for another 30 seconds.','Recipe_photos/e06f4a58-4f16-4f36-b5bd-db995fb2f168.jpg');
INSERT INTO "recipe_step" VALUES(103,10,9,'Sprinkle in the curry masala and stir it into the onions.','Recipe_photos/71c33087-bee8-452c-9ddc-665404998376.jpg');
INSERT INTO "recipe_step" VALUES(104,10,10,'Add the fish rolls and cook them all around in, say, 2 to 3 minutes.','Recipe_photos/91fb5518-a981-455f-8352-1cfc1ee5b458.jpg');
INSERT INTO "recipe_step" VALUES(105,10,11,'The masala will give those fish rolls a gorgeous colour.','Recipe_photos/1dffa897-f964-456e-9145-d872b786da0f.jpg');
INSERT INTO "recipe_step" VALUES(106,10,12,'Add the chill pepper and minced parsley. Crumble in the bouillon cube and pour in the water.','Recipe_photos/ff55e261-7040-49f8-995e-3b05a54ac610.jpg');
INSERT INTO "recipe_step" VALUES(107,10,13,'Bring it to a boil and let it simmer for 7 to 10 minutes, until the fish is done.','Recipe_photos/90ac728e-d85a-44a1-96ee-f207ee299a4b.jpg');
INSERT INTO "recipe_step" VALUES(108,10,14,'Spoon all these lovely cooking juices over the fish before serving and sprinkle some extra parsley for garnish.','Recipe_photos/03a68d23-2c12-4499-942b-0621a0ceb75b.jpg');
INSERT INTO "recipe_step" VALUES(109,11,1,'First things first; I’ve buttered and lined an 8 x 8 inch baking tray with baking paper.','Recipe_photos/c42bfc80-66cf-46dd-b05e-507612deb0fc.jpg');
INSERT INTO "recipe_step" VALUES(110,11,2,'Then I smashed about 7 oz pecan nuts I still had floating around my pantry. They could just as well have been walnuts, though. Leave a few nuts whole for garnish.','Recipe_photos/632a0f46-bd04-4c1d-91a0-d944210c2ecf.jpg');
INSERT INTO "recipe_step" VALUES(111,11,3,'I’ve set 3/4 cup of granulated sugar ready and grabbed 2 tbsp from it. Added the 2 tbsp to 10 oz (room temperature) cream cheese, along with 1/2 tsp vanilla extract. Use a little toddler hand for stirring (What do you mean child labour? He’s almost 3!) and set the bowl aside for a while so the sugar will dissolve.','Recipe_photos/e9d398d8-9fab-4fa9-8f08-a6fd81c7e616.jpg');
INSERT INTO "recipe_step" VALUES(112,11,4,'Place 4 oz of butter in a microwave-proof bowl, add about 4 oz chocolate, 2 tbsp cocoa powder and pop in the microwave until it melts. Stir well.','Recipe_photos/4e48d52d-deac-4ce3-9407-2b2d54e8eabb.jpg');
INSERT INTO "recipe_step" VALUES(113,11,5,'Break out your kitchenaid or hand-mixer and whisk 2 large eggs with the rest of the 3/4 cup sugar, until it’s all smooth and creamy.','Recipe_photos/a08cf9f2-86a3-46ba-be39-9c340f30ceb1.jpg');
INSERT INTO "recipe_step" VALUES(114,11,6,'Pour the butter/chocolate mix in there. Stir until it’s a pretty brown batter.','Recipe_photos/372bece8-5f25-4c6f-a023-8b030c3d8b43.jpg');
INSERT INTO "recipe_step" VALUES(115,11,7,'Sieve 3/4 cup self-rising flour right on top of the batter. Gotta love them action shots.','Recipe_photos/5f054aa2-282c-4563-81d2-d70c241fbbf2.jpg');
INSERT INTO "recipe_step" VALUES(116,11,8,'Throw in the pecan nuts and combine it all. A pretty turquoise spatula helps make the photos more interesting!','Recipe_photos/7b6f286d-714c-4c5a-bbd0-4f3e6914db71.jpg');
INSERT INTO "recipe_step" VALUES(117,11,9,'Pour half of the brownie batter into your baking tray. Level it.','Recipe_photos/c6927b9f-a41c-4e9a-b93b-d1f1deaa0376.jpg');
INSERT INTO "recipe_step" VALUES(118,11,10,'Top with the  cream cheese. You can add some fruit or even throw in a few jelly puddles here and there at this point. Strawberry jelly works really well! Just knock yourself out.','Recipe_photos/ec6bb43e-572d-49e1-af10-05e510a8b374.jpg');
INSERT INTO "recipe_step" VALUES(119,11,11,'And pour the other half of your brownie batter right on top of the cream cheese. Now you can swirl it, draw little puppets or animals in it, but I always leave it just like this. They come out real pretty that way.','Recipe_photos/b0eaaee1-a7c1-4eb2-afc6-182e576128d0.jpg');
INSERT INTO "recipe_step" VALUES(120,11,12,'Bake the cake in a preheated oven at 350F (180C) for 40 to 45 minutes!','Recipe_photos/cfeb3d7d-b8b1-47cb-bac3-b57ba1173848.jpg');
INSERT INTO "recipe_step" VALUES(121,11,13,'Grab 1 cup confectioners sugar.','Recipe_photos/e17e2c1e-1d20-407a-97b1-ef25d2af6b92.jpg');
INSERT INTO "recipe_step" VALUES(122,11,14,'Add it to the butter mix and start mixing. If it’s too thick, add more milk per tbsp.','Recipe_photos/b05e0b54-a30d-4f7a-b20b-1a735f8c684c.jpg');
INSERT INTO "recipe_step" VALUES(123,11,15,'Always taste what you’re doing. I do! And I have the thighs to show for it.','Recipe_photos/136a9fc6-17b5-4dc7-b8dd-ac01ef5494f6.jpg');
INSERT INTO "recipe_step" VALUES(124,11,16,'After 45 minutes my cake looked like this. Notice the cracks on top? Those are good. Those are really, really good.','Recipe_photos/54ddc91c-b6ba-4ae8-bf0f-38f17c162c75.jpg');
INSERT INTO "recipe_step" VALUES(125,11,17,'Let the cake cool off for at least 15 to 30 minutes now.','Recipe_photos/1a02dc6a-9f9a-4849-95dc-7207435f3429.jpg');
INSERT INTO "recipe_step" VALUES(126,11,18,'Spread the rest of the icing on top.','Recipe_photos/c1d57cc2-203f-4bf5-94b6-19a4df79b41d.jpg');
INSERT INTO "recipe_step" VALUES(127,11,19,'Garnish with the whole pecan nuts you kept behind.','Recipe_photos/704c5bdc-5e58-42a3-8851-5d6c735ed144.jpg');
INSERT INTO "recipe_step" VALUES(128,11,20,'Let the cake cool for several hours, preferably overnight, before cutting it in squares.','Recipe_photos/0d635592-6a65-416b-9b71-ee3d17a706ec.jpg');
INSERT INTO "recipe_step" VALUES(129,12,1,'Heat some olive oil and sauté the onion for a minute or 4, until translucent. Add the garlic and give it another minute before pouring in the diced tomatoes.','/media/Recipe_photos/41b87500-066b-4e7f-bd61-ee161197d753.jpg');
INSERT INTO "recipe_step" VALUES(130,12,2,'Season with a pinch of salt, loads of freshly cracked black pepper and a tsp or so Italian seasoning.','/media/Recipe_photos/0715fe3b-5a77-4867-85c5-0012afecffca.jpg');
INSERT INTO "recipe_step" VALUES(131,12,3,'Use a food processor and combine the olive oil, butter, a large handful of fresh basil and a few parsley leaves (1:3 ratio). Add the grated garlic and a spring onion.','/media/Recipe_photos/1f5fde30-ae07-4e6b-99bd-90129b4bbdd8.jpg');
INSERT INTO "recipe_step" VALUES(132,12,4,'Pulse until you have a lovely green flavored butter.','/media/Recipe_photos/8c66fa7b-0042-4735-afc5-7716d79243b8.jpg');
INSERT INTO "recipe_step" VALUES(133,12,5,'Slice it in half and spread the butter on the bottom half. Put the halves back together and bake them in a preheated oven at 375ºC (175ºF) 5 to 7 minutes.','/media/Recipe_photos/f8dafaba-0caf-45fc-8194-9952b19de1a8.jpg');
INSERT INTO "recipe_step" VALUES(134,12,6,'Boil the pasta in salted water according to the directions on the package.','/media/Recipe_photos/2dc13d63-05ed-4913-91d8-987aa43606ae.jpg');
INSERT INTO "recipe_step" VALUES(135,12,7,'After 30 minutes the tomato sauce will have cooked down quite a bit.','/media/Recipe_photos/de62fb92-76f9-4272-8842-3983f71da02d.jpg');
INSERT INTO "recipe_step" VALUES(136,12,8,'Stir in the mascarpone.','/media/Recipe_photos/a0977fb6-6410-40fa-8fee-20699d228292.jpg');
INSERT INTO "recipe_step" VALUES(137,12,9,'Taste to check the seasoning. Add more salt and pepper when needed.','/media/Recipe_photos/b3feeec7-166b-4746-9746-e268b7b51824.jpg');
INSERT INTO "recipe_step" VALUES(138,12,10,'Finish the sauce with lots of finely minced fresh basil.','/media/Recipe_photos/26161be9-bcf0-4163-890f-d77e5780befe.jpg');
INSERT INTO "recipe_step" VALUES(139,12,11,'Drain the pasta and stir it into the sauce adding a little of the cooking liquid as well.','/media/Recipe_photos/10be5277-3c4a-4f25-9760-9eb155ef626e.jpg');
INSERT INTO "recipe_step" VALUES(140,12,12,'Finish the pasta with lots of Parmesan curls and a few basil leaves. The mascarpone adds a wonderful richness and sweetness that really compliments the tanginess of the tomatoes. The garlic, onion and fresh basil perform the rest of the magic.  ','/media/Recipe_photos/4eabcd0d-018c-433d-9e8b-2047de574a82.jpg');
INSERT INTO "recipe_step" VALUES(141,12,13,'Finish the pasta with lots of Parmesan curls and a few basil leaves. The mascarpone adds a wonderful richness and sweetness that really compliments the tanginess of the tomatoes. The garlic, onion and fresh basil perform the rest of the magic.  ','Recipe_photos/4eabcd0d-018c-433d-9e8b-2047de574a82.jpg');
INSERT INTO "recipe_step" VALUES(142,13,1,'Coarsely chop the onion. Keep one slice aside for the sauce.','Recipe_photos/7cc19d90-df56-42f1-9365-fee8483b8286.jpg');
INSERT INTO "recipe_step" VALUES(143,13,2,'Get yourself a big bowl and pour the oil in there.','Recipe_photos/f900a0e8-0919-44d3-bd37-06ea48d59590.jpg');
INSERT INTO "recipe_step" VALUES(144,13,3,'This ABC kecap is sickly sweet. If you use a less sweet version feel free to add a tbsp (or two) brown sugar.','Recipe_photos/aaa5c49f-b712-4598-9d01-6991412dea98.jpg');
INSERT INTO "recipe_step" VALUES(145,13,4,'Squeeze the juice of half a lemon in there.','Recipe_photos/05197430-d3e6-4c29-9c98-c7d77f91c30a.jpg');
INSERT INTO "recipe_step" VALUES(146,13,5,'And grate the garlic and ginger in there as well.','Recipe_photos/f6d4c2ab-9d72-4187-abe5-ac81f6b354a0.jpg');
INSERT INTO "recipe_step" VALUES(147,13,6,'Add 2 tsp five-spice blend and 1/2 a tsp salt.','Recipe_photos/1e58b63b-419b-4899-be26-d96ea8ac3656.jpg');
INSERT INTO "recipe_step" VALUES(148,13,7,'Fit the pork nicely together in a casserole, or be stubborn and use a bowl. Divide the chopped onion over the meat.','Recipe_photos/b3f6170a-9847-43df-beaf-3533e5b82cbd.jpg');
INSERT INTO "recipe_step" VALUES(149,13,8,'Pour those lovely flavors all over the pork.','Recipe_photos/6e1653cb-3464-42d7-89b7-2a68945bdf35.jpg');
INSERT INTO "recipe_step" VALUES(150,13,9,'Dig your hands in there. Oh, come on, you know you want to! Cover the pork and let it marinate for an hour or 2.','Recipe_photos/cb80807f-6323-4faf-b0c6-6e69634441e2.jpg');
INSERT INTO "recipe_step" VALUES(151,13,10,'Transfer the pork to a large, heavy pan (with a lid).','Recipe_photos/01c28604-fa27-43b3-9ccb-b3aa9affdc01.jpg');
INSERT INTO "recipe_step" VALUES(152,13,11,'Pour every last drop of marinade in there as well. Including the onions.','Recipe_photos/fe99d3ab-55f2-4bcf-98d5-245e4ad501d9.jpg');
INSERT INTO "recipe_step" VALUES(153,13,12,'Add another 1/2 a tsp salt and pour enough water in there so that the pork is just about covered.','Recipe_photos/966e9a2a-0118-4b2e-891e-6989449a8dd3.jpg');
INSERT INTO "recipe_step" VALUES(154,13,13,'Simmer it, over low heat, with the lid on for 50 to 60 minutes. Flip the steaks halfway through the cooking process.','Recipe_photos/3e77195b-2f0f-4a33-b0d0-01be8f1b4dc9.jpg');
INSERT INTO "recipe_step" VALUES(155,13,14,'Finely mince the slice of onion you kept behind and grate the garlic. Heat the oil and sauté the onion and garlic until they soften. Don’t brown them.','Recipe_photos/be55ba6e-5012-4a99-a0ce-f7c6dded93a8.jpg');
INSERT INTO "recipe_step" VALUES(156,13,15,'Add the tomato paste and cook for a minute to sweeten it up.','Recipe_photos/561150c9-a822-4e25-b537-9c7c46ec48de.jpg');
INSERT INTO "recipe_step" VALUES(157,13,16,'Add the ground ginger, water, chili sauce, brown sugar, lemon juice, ketchup, vinegar and salt. Stir well.','Recipe_photos/ec467304-f145-4871-a79c-b3def58eda90.jpg');
INSERT INTO "recipe_step" VALUES(158,13,17,'Mix the corn starch with a tbsp water and pour it into the sauce. Simmer for an extra 2 to 3 minutes.','Recipe_photos/48b4e4db-447d-4750-97bd-5bf4d2498bea.jpg');
INSERT INTO "recipe_step" VALUES(159,13,18,'Dab the pork steaks completely dry with a clean paper towel. I don’t want you burning yourself later on.','Recipe_photos/eb40c491-ec26-49af-89e8-f9ff343419d6.jpg');
INSERT INTO "recipe_step" VALUES(160,13,19,'Heat the oil (355F˚-180C˚) and fry the steaks for roughly 2 minutes. They will almost blacken. Don’t be afraid, it’s all good. I think.','Recipe_photos/45b88f8f-6e6c-48b6-a98a-118a6e327efa.jpg');
INSERT INTO "recipe_step" VALUES(161,13,20,'Slice the pork. You’d think it would be dry after being abused like this: cooked, left to sit in hot water and deep-fried','Recipe_photos/a144d78e-cbe6-49e1-8155-1c76d26676ae.jpg');
INSERT INTO "recipe_step" VALUES(162,13,21,'Serve over white rice with acar campur (Indonesian pickled vegetables) and sauce.','Recipe_photos/5be2c898-1fbb-4648-ac45-8977c3e67681.jpg');
INSERT INTO "recipe_step" VALUES(163,14,1,'Mix 3 tbsp oil with the 2 tsp ranch seasoning and ground rosemary. This is the best fish marinade I’ve had in a long time.','Recipe_photos/26d1a85e-a2ea-4960-b61b-ff83b8bc51e2.jpg');
INSERT INTO "recipe_step" VALUES(164,14,2,'This was a little over 1 pound fresh cod tenderloin. You can use any kind of white fish you like.','Recipe_photos/bcfbd849-42c6-49e4-a89f-64895965da47.jpg');
INSERT INTO "recipe_step" VALUES(165,14,3,'They were two large pieces, I cut each one in half and brushed the marinade all around.','Recipe_photos/197c3ce5-17e5-47ab-b792-1366e2253d65.jpg');
INSERT INTO "recipe_step" VALUES(166,14,4,'Finely mince the onion and garlic.','Recipe_photos/01cfa9a4-ce15-4920-95e0-b823ec666c96.jpg');
INSERT INTO "recipe_step" VALUES(167,14,5,'Wash and dry your tomatoes. I used smaller plum tomatoes and cut each on in half. Cherry tomatoes go in whole.','Recipe_photos/d8838a4a-f94b-4bc5-81c5-facebc5cd8cb.jpg');
INSERT INTO "recipe_step" VALUES(168,14,6,'Heat a tbsp oil and sauté the shallot for 2 minutes. Add the garlic and tomatoes, give it 2 more minutes and season lightly with salt and pepper.','Recipe_photos/c9db171d-77c4-44f4-bdfb-b3fc292a0e42.jpg');
INSERT INTO "recipe_step" VALUES(169,14,7,'Place the pieces of fish in a casserole.','Recipe_photos/06acc58e-c16e-4f6b-8f28-ec1dbd83c4e1.jpg');
INSERT INTO "recipe_step" VALUES(170,14,8,'Place the tomatoes in with the cod and spoon the onions over the tomatoes.','Recipe_photos/a2317a9b-e49a-4037-9bf6-ae0e8f007da4.jpg');
INSERT INTO "recipe_step" VALUES(171,14,9,'Mix the panko with the kosher salt.','Recipe_photos/9fe20648-f823-4e69-bc93-b3ef4ab70c7b.jpg');
INSERT INTO "recipe_step" VALUES(172,14,10,'This is me chopping fresh herbs at warp speed!','Recipe_photos/1615cc09-0694-4170-9474-f3cbd51e9acf.jpg');
INSERT INTO "recipe_step" VALUES(173,14,11,'Add the fresh herbs to the panko, mix it up and divide it over the fish. Drizzle a tiny bit of oil on top of the panko.','Recipe_photos/003c7f6d-04c9-47cf-a1f8-853b8ef1f081.jpg');
INSERT INTO "recipe_step" VALUES(174,14,12,'Fish should swim, so serve this with a nice Sauvignon blanc, not too cold.','Recipe_photos/c01c521b-8ffa-4b2c-ba99-d91507e8ca29.jpg');
INSERT INTO "recipe_step" VALUES(175,15,1,'Here are the evil-doers responsible for my new soup addiction.','Recipe_photos/b5d58782-e6ca-4f5a-bf27-df52ac83fc43.jpg');
INSERT INTO "recipe_step" VALUES(176,15,2,'1 pound chicken breasts, though I’m sure you can make this soup with a cut up fryer as well.','Recipe_photos/9132c48c-aa25-4240-92b7-781664e8db25.jpg');
INSERT INTO "recipe_step" VALUES(177,15,3,'Mince the onions.','Recipe_photos/d77605b4-cc35-4890-9c92-b1b5c15c8f0e.jpg');
INSERT INTO "recipe_step" VALUES(178,15,4,'Add the small onion to a big pot, grate the garlic in there and add the bay leaves. Pour 6 cups of water in there and add the chicken. Bring it to a boil, let the chicken simmer for 10 minutes, turn off the heat and let the chicken stay in the hot water for another 15 minutes. Strain the liquid.','Recipe_photos/27cf2a01-556e-4bf6-a66d-bd46a8e7c536.jpg');
INSERT INTO "recipe_step" VALUES(179,15,5,'Keep the liquid nearby and let the chicken cool off a little. It’ll be really tender and succulent now.','Recipe_photos/8c3a61bf-7665-4da5-897f-7e5b3873ff6d.jpg');
INSERT INTO "recipe_step" VALUES(180,15,6,'Peel the skin off of the tomatoes, squeeze out the seeds and chop them up.','Recipe_photos/4f94895f-9a94-494a-a4c6-85ddc6a43b90.jpg');
INSERT INTO "recipe_step" VALUES(181,15,7,'Tried to get my hands on arroz dorado but couldn’t find it. Opted for brown rice in stead.','Recipe_photos/834ec968-9bb8-4008-bc43-7037859cde02.jpg');
INSERT INTO "recipe_step" VALUES(182,15,8,'Rinse about 1/2 to 2/3 cup chick peas.','Recipe_photos/010193eb-9b59-4dc3-ac1a-b92598797a13.jpg');
INSERT INTO "recipe_step" VALUES(183,15,9,'Heat 2 tbsp olive oil in a soup pan.','Recipe_photos/2ba1f26d-ffb1-4798-989d-c08b570d167d.jpg');
INSERT INTO "recipe_step" VALUES(184,15,10,'Sauté the onion for 3 minutes, add the tomatoes and give it another 3 minutes before adding the rice. Cook everything for 3 more minutes.','Recipe_photos/f03b6983-9c44-4617-a3e5-fb15d9d10609.jpg');
INSERT INTO "recipe_step" VALUES(185,15,11,'Add 1/2 tsp Mexican oregano.','Recipe_photos/94f28444-0a2a-4f3a-a489-48b9f9e92c3e.jpg');
INSERT INTO "recipe_step" VALUES(186,15,12,'Pour in the chicken broth and add the bouillon cubes. Simmer the soup for 10 minutes.','Recipe_photos/79024d49-6ea4-4660-8c04-639ec7d6ee98.jpg');
INSERT INTO "recipe_step" VALUES(187,15,13,'I know I was supposed to use 1 whole chipotle per bowl, but I decided to slice 2 chipotles in strips in stead.','Recipe_photos/115f7383-10d4-4141-91af-35b87edc5361.jpg');
INSERT INTO "recipe_step" VALUES(188,15,14,'Shred the chicken.','Recipe_photos/f66e08d8-df60-4338-b26f-2e73e2251ecf.jpg');
INSERT INTO "recipe_step" VALUES(189,15,15,'Add the chipotles, 1 or 2 tsp adobo sauce, the chick peas and shredded chicken.','Recipe_photos/523e453a-af07-4292-b7b4-cd37d8369f77.jpg');
INSERT INTO "recipe_step" VALUES(190,15,16,'Stir it, check the seasoning -mine needed extra salt- and simmer the soup for 10 to 15 more minutes. Until the rice is fully cooked.','Recipe_photos/d58e74f9-5ef5-4810-9668-79491ea1cbbb.jpg');
INSERT INTO "recipe_step" VALUES(191,15,17,'Finely mince coriander (or parsley, as I do), cube the avocado and slice the limes in wedges. Caldo Tlalpeño','Recipe_photos/81ea9216-31c7-4219-ac49-d574ded2cbbc.jpg');
INSERT INTO "recipe_step" VALUES(192,15,18,'Stir the cilantro into the soup.','Recipe_photos/256ba954-bc16-4df6-9bf5-d4707657164e.jpg');
INSERT INTO "recipe_step" VALUES(193,15,19,'Do yourself a big favor and serve cheese quesedillas on the side.','Recipe_photos/d0c4bde7-d0b5-41a6-9a1c-c6a46824995f.jpg');
INSERT INTO "recipe_step" VALUES(194,16,1,'2 pounds stewing steak, cut in roughly 1 inch pieced and pat dry with a paper towel. Meat will not brown properly if damp.','Recipe_photos/feca33a9-05c9-4450-8bd4-86a827b45f33.jpg');
INSERT INTO "recipe_step" VALUES(195,16,2,'4 big onions. This stew relies heavily on onion for flavor. Happy sigh.','Recipe_photos/c0db78de-4010-450b-ae5e-f7b45c955b4b.jpg');
INSERT INTO "recipe_step" VALUES(196,16,3,'Give them a rough chop.','Recipe_photos/0ec84440-8bec-489e-b553-4d97c8de3cbf.jpg');
INSERT INTO "recipe_step" VALUES(197,16,4,'Season the beef cubes with a generous amount of salt and freshly cracked black pepper.','Recipe_photos/f2788ffe-3f07-42c1-b9aa-dd77bd8134cb.jpg');
INSERT INTO "recipe_step" VALUES(198,16,5,'Heat some butter and sauté the beef cubes a few pieces at a time until nicely browned on all sides. Don’t overcrowd the pan or they won’t brown properly.','Recipe_photos/5f278eb7-e1a1-44c7-82fe-65588b99391b.jpg');
INSERT INTO "recipe_step" VALUES(199,16,6,'While browning the beef, you also peel the apples. I’ve used Goudreinette, a slightly sweet and more sour apple. Dice them.','Recipe_photos/f0f27be1-5cfd-4eda-bb59-d87699f9e542.jpg');
INSERT INTO "recipe_step" VALUES(200,16,7,'As soon as the last batch has browned, transfer all the meat to a big Dutch oven. Provided you didn’t start out with a Dutch oven in the first place.','Recipe_photos/4fabffc3-8aee-47e9-be55-e9824a4cc92e.jpg');
INSERT INTO "recipe_step" VALUES(201,16,8,'Cook the onions. Oh man, there’s nothing I love more than the smell of cooking onions. Okay, the scent of my baby’s hair, but that’s it.','Recipe_photos/cec0a2c8-0cca-438c-9a2f-e018cefe881b.jpg');
INSERT INTO "recipe_step" VALUES(202,16,9,'They don’t have to be cooked all the way. Just a little browned and translucent is fine. Add them to the beef cubes.','Recipe_photos/d7f5eacc-8f59-42cf-9251-26cc377fdd03.jpg');
INSERT INTO "recipe_step" VALUES(203,16,10,'Spoon 3 tbsp flour on top, stir in and let it cook for a minute.','Recipe_photos/4f277b5d-a379-436c-a85a-d28ab6cafa5d.jpg');
INSERT INTO "recipe_step" VALUES(204,16,11,'Stir in the tomato paste and give it an extra minute to sweeten up.','Recipe_photos/541e651b-2854-498b-9a05-55d9ecd28387.jpg');
INSERT INTO "recipe_step" VALUES(205,16,12,'Pour in just enough water to submerge the meat.','Recipe_photos/a72f266e-e23d-4439-bc42-27af7e4881a6.jpg');
INSERT INTO "recipe_step" VALUES(206,16,13,'This will end out to be somewhere around 4 to 5 cups.','Recipe_photos/51914fb8-a659-436b-914d-b6e04fe73bce.jpg');
INSERT INTO "recipe_step" VALUES(207,16,14,'Add the diced apples, thyme (use more thyme if you like) cloves, peppercorns and beef bouillon cubes.','Recipe_photos/f17dd8c6-a9e2-4b90-ae99-1603b05cc48e.jpg');
INSERT INTO "recipe_step" VALUES(208,16,15,'Spike the stew with 2 tbsp molasses (or appelstroop). When you’re in the Netherlands I urge you to add a crumbled up piece of ontbijtkoek. It’s divine.','Recipe_photos/3fbd027f-e680-461d-a124-3dea9f78f996.jpg');
INSERT INTO "recipe_step" VALUES(209,16,16,'Two or three bay leaves and one fresh rosemary sprig. I think the rosemary makes a huge difference.','Recipe_photos/5a0d99e0-df55-4adb-acd6-22658f5a4cd4.jpg');
INSERT INTO "recipe_step" VALUES(210,16,17,'Depending on how salty the bouillon cubes are add a little extra salt. Remember you can always add more later. I stuck to a tsp.','Recipe_photos/13943bc9-85b0-4168-bbf9-d94c1b30523c.jpg');
INSERT INTO "recipe_step" VALUES(211,16,18,'Bring it to a boil. Put the lid on and forget all about it for the next, say 2 and a half to 3 hours. You can stir now and then if you really want to, though.','Recipe_photos/22dcd0f9-e4d7-4d02-a9fb-95344e83cc21.jpg');
INSERT INTO "recipe_step" VALUES(212,16,19,'Here’s what mine looked like after 3 hours. Still a bit high on liquid and we want that to be more of a gravy, so I took the lid off and let it cook down.','Recipe_photos/149525f4-0b86-4c1c-b84f-073c5262ccc9.jpg');
INSERT INTO "recipe_step" VALUES(213,16,20,'Here’s my personal preference. I use my masher to break up the beef. Then I let it simmer for another 30 to 45 minutes until the gravy had the consistency I was looking for. Check the seasoning and add pepper! A generous amount. Hachee','Recipe_photos/8ae30268-f1c3-4390-b24c-8143a272aa15.jpg');
INSERT INTO "recipe_step" VALUES(214,16,21,'This was a quick shot taken right at my dinner table: mashed up aromatic beef, gravy, red cabbage and fluffy mashed potatoes. This is the life.','Recipe_photos/3f449354-0017-4641-bf30-11ff799ba4c1.jpg');
INSERT INTO "recipe_step" VALUES(215,17,1,'I’m dying to know what you think of this recipe, so let’s get started. Peel, wash and dice the potatoes. Cut bigger potatoes in four and smaller ones in half. Boil them in salted water until tender.','Recipe_photos/3b27b414-d2a5-4320-b9e9-4e2e5d1fad55.jpg');
INSERT INTO "recipe_step" VALUES(216,17,2,'Apples and sauerkraut is like a match made in culinary heaven. They really compliment each other. Peel and finely mince two smaller or 1 big sweet apple.','Recipe_photos/1fb59a1c-3b91-44a9-8c38-31376f4f281a.jpg');
INSERT INTO "recipe_step" VALUES(217,17,3,'Drain the sauerkraut.','Recipe_photos/ffd30159-a6bc-4b51-a426-24205ba1db45.jpg');
INSERT INTO "recipe_step" VALUES(218,17,4,'Heat a tiny drop of butter and sauté the apple for 3 minutes. Until slightly soft.','Recipe_photos/88e7782d-722c-41ff-9728-cf46a779158f.jpg');
INSERT INTO "recipe_step" VALUES(219,17,5,'Add the sauerkraut, sprinkle a generous amount of black pepper in there as well, combine everything, pop the lid on and simmer over really low heat. 5 minutes will do.','Recipe_photos/8f0f4703-e6fc-485b-8ed3-a0bf3a1f1609.jpg');
INSERT INTO "recipe_step" VALUES(220,17,6,'Grate two cups of your favorite cheese. Make sure you go with a cheese that melts well. I opted for Gouda, but don’t I always? I know, it’s pathetic how predictable I am! Sauerkraut, Apples & Ham Rolls','Recipe_photos/1a5425a4-42db-44cb-bd52-fc515e29860c.jpg');
INSERT INTO "recipe_step" VALUES(221,17,7,'As soon as your taters are done, drain them, steam-dry them for a minute or two and transfer them to an oven dish.','Recipe_photos/9625f48c-7505-4855-851c-c8f7f8593445.jpg');
INSERT INTO "recipe_step" VALUES(222,17,8,'Glorious ham. I love ham. Everyone loves ham, or at least they should. Has to be all the MSG they stuff it with. We should probably name it HamSG. Top each slice of ham with a royal amount of sauerkraut and start rolling those babies. Sauerkraut, Apples & Ham Rolls','Recipe_photos/04672dd9-1d8b-4d31-b93a-935dd015fff5.jpg');
INSERT INTO "recipe_step" VALUES(223,17,9,'Heat 2 oz of butter. Sprinkle 4 tbsp all-purpose flour in there, stir and cook over low heat for 2 minutes. Stay nearby and stir, if you mess up the roux you’re back to square one. Don’t say I didn’t warn you!','Recipe_photos/fea3415e-404c-4af6-9d2e-608a4e691fb8.jpg');
INSERT INTO "recipe_step" VALUES(224,17,10,'Pour in 1 1/2 cup of milk, stir and bring to a boil. As soon as the milk heats up, the sauce will thicken. Usually this will give you the exact consistence you need, but if your sauce ends up too thick just add a little splash of milk. Simmer the sauce for 2 minutes.','Recipe_photos/9a03a037-299c-47d1-8139-064c6f28253c.jpg');
INSERT INTO "recipe_step" VALUES(225,17,11,'See where we’re going with this? Add the grated cheese, stir until it melts and season the sauce with salt, pepper and a touch of mace or nutmeg.','Recipe_photos/b8773aeb-12ef-4fd6-98d7-8bea2b86a9db.jpg');
INSERT INTO "recipe_step" VALUES(226,17,12,'Spoon half the sauce over the potatoes.','Recipe_photos/43993ff5-31d8-43e6-a2eb-aac218027904.jpg');
INSERT INTO "recipe_step" VALUES(227,17,13,'Place the ham, sauerkraut & apple rolls on top and spoon the other half of the sauce over the rolls.','Recipe_photos/fcd93758-4d87-4a47-b760-1a867e1c7b76.jpg');
INSERT INTO "recipe_step" VALUES(228,17,14,'Until it looks like some sort of culinary and rather creamy piece of art. The only enhancement it still needs, in my humble opinion, is a big fork to devour it with.','Recipe_photos/bdd820b7-e5d4-4f92-a8ee-0614aba5f95c.jpg');
INSERT INTO "recipe_step" VALUES(229,17,15,'Take it out of the oven and revel in its beauty.','Recipe_photos/31673b60-9795-4d00-86f6-86b497ce7261.jpg');
INSERT INTO "recipe_step" VALUES(230,18,1,'We’re going to drain the whey so we’re left with a really thick yoghurt. Place a colander over a big bowl. The colander should not reach the bottom of the bowl.','Recipe_photos/9dee02b9-519f-496c-9c28-0de39d83bf01.jpg');
INSERT INTO "recipe_step" VALUES(231,18,2,'And put a (clean) tea towel in there.','Recipe_photos/2c20fc74-dbc1-4045-966a-d8cadc36731f.jpg');
INSERT INTO "recipe_step" VALUES(232,18,3,'Pour the yoghurt into the towel. You could also do this with Turkish or Greek yoghurt. Lightly fold the towel over the yoghurt.','Recipe_photos/d5e75f2d-57c7-4abd-a3e1-8c99e8e7dbef.jpg');
INSERT INTO "recipe_step" VALUES(233,18,4,'The whey will start dripping out immediately.','Recipe_photos/1313ff02-b1fa-4638-90b2-28f65e0c7432.jpg');
INSERT INTO "recipe_step" VALUES(234,18,5,'I went for 4 hours or my light would’ve been gone, normally I would have taken my time. The hangop could be much thicker, but you get the idea, right? Transfer the drained yoghurt to a big bowl.','Recipe_photos/ba2ca2f4-78f4-4897-9a26-a9e8ae9af6b7.jpg');
INSERT INTO "recipe_step" VALUES(235,18,6,'Hangop  Here’s the whey that came out of it. I’ve been told it makes for a great soup base. Have to try that sometime.','Recipe_photos/5403ad86-ae57-479b-8bf8-17ee84f9fa95.jpg');
INSERT INTO "recipe_step" VALUES(236,18,7,'This was the first time I made whipped cream in my new gadget! One cup of cream. Don’t use the canned stuff, it’s gross!','Recipe_photos/a7138920-a160-43ae-99ab-041bc450d99b.jpg');
INSERT INTO "recipe_step" VALUES(237,18,8,'Combined with sugar. Use more sugar than you normally would, so you’ll also sweeten the yoghurt. I opted for 5 tbsp, mildly sweet, use more if you have a sweet tooth. Hangop','Recipe_photos/f56884dc-b183-437f-8ef7-b3cfb57f5e44.jpg');
INSERT INTO "recipe_step" VALUES(238,18,9,'There. Whipped cream.','Recipe_photos/826bdf59-1698-407c-94cf-179313735b24.jpg');
INSERT INTO "recipe_step" VALUES(239,18,10,'Please, don’t use vanilla extract in hangop. It’s a venial sin (pretty low on the scale, but still). You need the real deal, a big vanilla bean. Open up the pod and scrape the insides to get the vanilla seeds out.','Recipe_photos/21c32c76-fd61-4122-9709-bb850a4d4f5c.jpg');
INSERT INTO "recipe_step" VALUES(240,18,11,'Add the vanilla seeds to the yoghurt and stir it in.','Recipe_photos/81895d52-4fe9-4e18-8fa9-241f357c4175.jpg');
INSERT INTO "recipe_step" VALUES(241,18,12,'I love seeing those black specks that give away real vanilla was used.','Recipe_photos/d1ea48e8-9211-42f0-b166-740f939bdf28.jpg');
INSERT INTO "recipe_step" VALUES(242,18,13,'Carefully spoon in the whipped cream.','Recipe_photos/c779f2b4-9aa2-4d15-86a9-2320b79e0f6c.jpg');
INSERT INTO "recipe_step" VALUES(243,18,14,'And this is Hangop. Literally translated it means hang up.','Recipe_photos/8cee147e-b3d1-412a-b18a-5a31197c4257.jpg');
INSERT INTO "recipe_step" VALUES(244,18,15,'I prefer topping mine with fresh fruit. But you can top it with all sorts of things: nuts and honey, warm fruit, fruit soaked in alcohol, caramelized walnuts, compotes. Anything you like.','Recipe_photos/e5b32a50-8dde-45c9-a354-2e0f6efcf115.jpg');
INSERT INTO "recipe_step" VALUES(245,12,0,'All my sauces start with a finely minced onion and garlic. I’m boring, I know.','/media/Recipe_photos/bcf9eee8-b2fa-47dc-94e6-596d88a5a9b4.jpg');
INSERT INTO "recipe_step" VALUES(246,19,1,'Free-range chicken. I chose legs because they have more flavour than the breast.','Recipe_photos/b3be6172-08e6-4c0f-aab9-0ce74ddbbfa1.jpg');
INSERT INTO "recipe_step" VALUES(247,19,2,'One medium onion cut in 4 pieces.','Recipe_photos/456d1284-05c0-4291-a240-6937b4771e85.jpg');
INSERT INTO "recipe_step" VALUES(248,19,3,'Place the legs in a large pot and add the onion parts.','Recipe_photos/d60de67a-5bb9-461a-a8f6-f64938bdcad1.jpg');
INSERT INTO "recipe_step" VALUES(249,19,4,'Add the garlic, cinnamon stick, pepper, nutmeg, bouillon cube, kosher salt and water.','Recipe_photos/45205973-248d-4d56-9f46-82de8db501f0.jpg');
INSERT INTO "recipe_step" VALUES(250,19,5,'And finally, you drop 2 bay leaves in there.','Recipe_photos/ab563eef-aedd-4f38-b727-bc1193991027.jpg');
INSERT INTO "recipe_step" VALUES(251,19,6,'Bring it to a boil, lower the heat and let the chicken simmer for 35 to 40 minutes.','Recipe_photos/15eca11e-871b-4b60-8e34-605ccefdcaea.jpg');
INSERT INTO "recipe_step" VALUES(252,19,7,'Transfer the chicken legs to a plate, let them cool off and hold on to the broth. We’ll be needing it.','Recipe_photos/7d88ad84-ab42-4502-9140-545f326d4781.jpg');
INSERT INTO "recipe_step" VALUES(253,19,8,'Combine the sumac…','Recipe_photos/869a36dd-4f96-4afb-ba91-bb46d5b6a7d7.jpg');
INSERT INTO "recipe_step" VALUES(254,19,9,'… with the remaining ingredients listed under Spice mix.','Recipe_photos/b038eec5-2fa1-4e3b-a7e2-aa2639a156ba.jpg');
INSERT INTO "recipe_step" VALUES(255,19,10,'Use your food processor or spice mill to give it a quick grind.','Recipe_photos/09311264-f988-424e-9895-71e2b03a980f.jpg');
INSERT INTO "recipe_step" VALUES(256,19,11,'Cut 2 onions in quart-rings.','Recipe_photos/c0d585a8-9bc5-48bb-9965-0f2bbf392efc.jpg');
INSERT INTO "recipe_step" VALUES(257,19,12,'Heat the oil. I’m strangely fascinated by this photo and have no idea why.','Recipe_photos/80f1a481-778e-4cd9-b891-3ddd751db915.jpg');
INSERT INTO "recipe_step" VALUES(258,19,13,'Cook the onions over low to medium heat for 10 to 15 minutes.','Recipe_photos/ff238a40-da3e-47d7-bdfc-08ebb98dbdfa.jpg');
INSERT INTO "recipe_step" VALUES(259,19,14,'Pick the chicken meat off the bones. I’m real good at delegating stuff like this! Kay''s Musakhan','Recipe_photos/cda787cc-a713-48d3-8dcc-03806b65dfd4.jpg');
INSERT INTO "recipe_step" VALUES(260,19,15,'Mix 3/4 of the spice mix over the chicken and stir it in.','Recipe_photos/4c887958-8660-4d52-ba6d-0a5ae2542417.jpg');
INSERT INTO "recipe_step" VALUES(261,19,16,'Add the remaining spice mix to the onions.','Recipe_photos/d1d55bb2-569b-47f8-819f-5fef52cd1d94.jpg');
INSERT INTO "recipe_step" VALUES(262,19,17,'Here’s where the broth comes in: you lightly brush the crust with the broth. Don’t soak it, just lightly brush it atop the crust.','Recipe_photos/d40eb503-954d-44ed-b348-751183c7ba97.jpg');
INSERT INTO "recipe_step" VALUES(263,19,18,'Place the bread in a baking tray, top with the chicken and onions.','Recipe_photos/1e6b126f-eb28-4895-afb2-abae6dda042d.jpg');
INSERT INTO "recipe_step" VALUES(264,19,19,'This is my kinda comfort food.','Recipe_photos/0aa892f9-318d-4f9c-a486-e0061ace8035.jpg');
INSERT INTO "recipe_step" VALUES(265,20,1,'Peel the ginger en coarsely chop it. I’ll be using my immersion blender later on, you can also finely chop everything if you want to skip that step.','Recipe_photos/b8799c38-fe1a-41b0-9c02-a4745540da7e.jpg');
INSERT INTO "recipe_step" VALUES(266,20,2,'Slap the lemongrass on your countertop to release some of the oils, and chop it along with the garlic and shallot.','Recipe_photos/4fe82d74-7343-4f41-b979-38f6949093f0.jpg');
INSERT INTO "recipe_step" VALUES(267,20,3,'Just a few jalapeño slices (I used the seeds as well). Heat the oil and stir-fry everything for 2 to 3 minutes.','Recipe_photos/e79eb9b7-ce62-47c9-b326-bebc4044b6c1.jpg');
INSERT INTO "recipe_step" VALUES(268,20,4,'Add the pepper and give it another minute before pouring in 2 tbsp water and sprinkling in the sugar. Lemongrass Ginger Marinade','Recipe_photos/2b1e0d12-fae7-4dba-bd81-edbdc653aaef.jpg');
INSERT INTO "recipe_step" VALUES(269,20,5,'Stir and simmer until the sugar turns light beige.','Recipe_photos/b5fdaba7-8a0a-43dc-9dba-6f7086d808e9.jpg');
INSERT INTO "recipe_step" VALUES(270,20,6,'That’s your cue to pour in the remaining water, light soy sauce, fish sauce and mirin.','Recipe_photos/fd1749d0-c703-4bf8-8a53-6cf68128bf5d.jpg');
INSERT INTO "recipe_step" VALUES(271,20,7,'I then used my immersion blender but you can use a regular blender or food processor as well. Let the marinade cool off.','Recipe_photos/9a2789c5-a092-4fc2-be9c-f67707a231dd.jpg');
INSERT INTO "recipe_step" VALUES(272,20,8,'Marinated them for about two hours in my fridge and scraped off the excess ingredients before grilling them 2 minutes over direct heat on each side. (Big green egg 400F – 200C).','Recipe_photos/f587b521-cd49-4d0b-9140-19603685443c.jpg');
INSERT INTO "recipe_step" VALUES(273,20,9,'Steak this simple never tasted so good!','Recipe_photos/260f94ce-736f-48fc-8baf-7967d1244763.jpg');
INSERT INTO "recipe_step" VALUES(274,21,1,'In the food processor you combine all garlic harissa aoili ingredients except for the oil. Squeeze in the garlic.','Recipe_photos/01b0b43d-5237-4192-8782-0e10a9e63230.jpg');
INSERT INTO "recipe_step" VALUES(275,21,2,'Pulse a few times to combine.','Recipe_photos/4e130d95-386b-4b77-ba29-32cfa27c611d.jpg');
INSERT INTO "recipe_step" VALUES(276,21,3,'Let your food processor spin and slowly pour in the oil in a really thin stream. Seriously, no big splashes or you’ll never get an emulsion going.','Recipe_photos/59f387a8-4876-4366-95a0-cf82e880b0e4.jpg');
INSERT INTO "recipe_step" VALUES(277,21,4,'Halfway through the process you’ll see it transforming from a yellow glob into a real mayonnaise. That’s your yay moment.','Recipe_photos/22f6ab93-103a-435b-9eff-baa5d5f1fae7.jpg');
INSERT INTO "recipe_step" VALUES(278,21,5,'Transfer the mayo to a bowl and add the harissa.','Recipe_photos/d2257f9f-fa30-451d-828e-574b91fdf202.jpg');
INSERT INTO "recipe_step" VALUES(279,21,6,'Stir it in and check the seasoning. You might want to add a bit more salt.','Recipe_photos/92743fc7-f6e2-4f1b-9921-29fb6d740dcb.jpg');
INSERT INTO "recipe_step" VALUES(280,21,7,'The stuff. The pistolets could have easily been a baguette and the mustard greens could have been arugula as well! I’m so easy going.','Recipe_photos/0b99c411-4095-4d25-9285-3e0e420d5114.jpg');
INSERT INTO "recipe_step" VALUES(281,21,8,'Heat your grill and give the sausages and onions 5 to 6 minutes, flipping them now and then.','Recipe_photos/9750f2c5-3474-44fc-aaf2-5c5ed12f6126.jpg');
INSERT INTO "recipe_step" VALUES(282,21,9,'I love a good sausage sandwich!','Recipe_photos/bcc55231-0a38-414f-a5e7-f47633f8fde5.jpg');
INSERT INTO "recipe_step" VALUES(283,22,1,'The chicken is optional. If you want to make the rice as a side dish, you can opt to leave it out. Finely mince it and season the pieces with salt and pepper.','Recipe_photos/699a0be0-e90e-4fa3-95e3-28d36c4b8e31.jpg');
INSERT INTO "recipe_step" VALUES(284,22,2,'Finely mince the onion, bell pepper, Jalapeno, and tomatoes.','Recipe_photos/a2a6246c-37c6-4882-acd8-3e6222e7c850.jpg');
INSERT INTO "recipe_step" VALUES(285,22,3,'Heat 2 tbsp oil and cook the chicken until it loses its rawness. It can still be raw on the inside. Remove it from the pan.','Recipe_photos/78d7a8fa-1fb2-46db-a0f2-985c0c8c822b.jpg');
INSERT INTO "recipe_step" VALUES(286,22,4,'Heat the remaining oil and sauté the onion, pepper and jalapeno until the onion is translucent.','Recipe_photos/a1925e00-951f-470a-a33e-dc5e0b15b92c.jpg');
INSERT INTO "recipe_step" VALUES(287,22,5,'Then add the tomatoes and give it 3 more minutes to simmer.','Recipe_photos/ee025531-e1ea-46d5-944c-9d3ba0addadb.jpg');
INSERT INTO "recipe_step" VALUES(288,22,6,'Sprinkle the cumin, paprika, pepper, and turmeric over the vegetables and cook them for 45 seconds.','Recipe_photos/e197fe7a-e6fb-499e-afbb-48981bae6c83.jpg');
INSERT INTO "recipe_step" VALUES(289,22,7,'Pour in the rice (mine was Surinam rice) and stir well. Let the rice absorb the flavors for about 2 minutes.','Recipe_photos/b871cc7d-895f-4dac-a15c-2b71d45b83b4.jpg');
INSERT INTO "recipe_step" VALUES(290,22,8,'In with the oregano.','Recipe_photos/cee0ee2e-cb8b-4a62-bac0-2aadb38e6953.jpg');
INSERT INTO "recipe_step" VALUES(291,22,9,'Add the chicken and pour in the broth. I added a dash of cayenne because my jalapeno wasn’t hot and my broth wasn’t salty so I threw in a tsp salt as well.','Recipe_photos/d62c701a-10d1-4eed-8588-0ec5fa18faea.jpg');
INSERT INTO "recipe_step" VALUES(292,22,10,'Bring this to a boil, pop the lid on and simmer (no peeking!) for 25 minutes.','Recipe_photos/26a072c0-bd1a-4e11-99d4-19f01bc33905.jpg');
INSERT INTO "recipe_step" VALUES(293,22,11,'This is what it looks like after 25 minutes. Turn off the heat, stir well and leave it be for another 10 minutes. Check the seasoning and garnish with finely minced flat-leaf parsley. Seriously Seasoned Rice','Recipe_photos/e34d970b-c4a8-4dac-8464-728bd5987f38.jpg');
INSERT INTO "recipe_step" VALUES(294,22,12,'This is the kind of food I eat in a bowl, with a spoon, curled up on the couch underneath my stripy plaid while watching a chick flick.','Recipe_photos/803e58a6-3617-4828-819b-f3d62e02fef7.jpg');
INSERT INTO "recipe_step" VALUES(295,23,1,'See how the edge has an inward curve? It catches all the vegetables, flips them over and drops them back into the pan.','Recipe_photos/f87118dc-9629-4dc1-8e38-6ffefa577b74.jpg');
INSERT INTO "recipe_step" VALUES(296,23,2,'Finely mince the chill pepper (use less if you’re not into spicy foods), bell pepper, onion and garlic.','Recipe_photos/b396dc0a-ccbe-4ec2-9a8c-c50005f8199f.jpg');
INSERT INTO "recipe_step" VALUES(297,23,3,'Heat 1 or 2 tsp oil and lightly sauté the vegetable mix until the onions turn translucent.','Recipe_photos/449456d1-8c33-44b7-a212-65894a54807c.jpg');
INSERT INTO "recipe_step" VALUES(298,23,4,'Add the spices and cook them along to get the flavours going.','Recipe_photos/bca8ca3a-fc0d-4d03-b5fa-db24f204709f.jpg');
INSERT INTO "recipe_step" VALUES(299,23,5,'Pour in the soy sauce, molasses, brown sugar, chill sauce, water and stir well.','Recipe_photos/b34f2a88-b56e-4579-97a6-413e516d2586.jpg');
INSERT INTO "recipe_step" VALUES(300,23,6,'Bring it to a boil and simmer over low heat for about 5 minutes.','Recipe_photos/27a53028-d95b-4299-9514-42b8ca0e2e47.jpg');
INSERT INTO "recipe_step" VALUES(301,23,7,'Start with 1 tbsp cornstarch (whisked into a tbsp water) and stir it in. The sauce should get pretty thick, add 1/2 tbsp more cornstarch if you need it.','Recipe_photos/de7c2e81-f730-4d44-93dc-7010b07153d4.jpg');
INSERT INTO "recipe_step" VALUES(302,23,8,'Turn off the heat and stir in 2 (or 3) tbsp lemon juice to finish the sauce. It balances the flavours.','Recipe_photos/3e7ace70-79a8-458b-b43a-95be3336dcb1.jpg');
INSERT INTO "recipe_step" VALUES(303,23,9,'Peel the carrot and thinly slice it. Trim the beans and wash them. Cut the peppers in strips and chop the spring onions.','Recipe_photos/977162e6-b318-4667-aa1e-33e04c4c6bcc.jpg');
INSERT INTO "recipe_step" VALUES(304,23,10,'Cut the onion in wedges and peel the layers apart. Clean and slice the mushrooms and cut the chicken breast into thin strips and lightly season them with salt.','Recipe_photos/ae41cc60-d2fa-49a6-b64c-fc8c553c18f2.jpg');
INSERT INTO "recipe_step" VALUES(305,23,11,'Hot pan, cool oil. Always keep that in mind. Heat the pan, add a tbsp or so oil, swirl oil and start stir-frying straight away.','Recipe_photos/d59be0b1-3f07-45d9-9ac7-d42f80c4ad74.jpg');
INSERT INTO "recipe_step" VALUES(306,23,12,'Always start with the protein. First you stir-fry the chicken and keep it moving so it won’t burn. Transfer the chicken to a plate.','Recipe_photos/9141273f-154c-44fd-af80-df9d70a15bc7.jpg');
INSERT INTO "recipe_step" VALUES(307,23,13,'I stir-fried the beans for about 3 to 4 minutes until they started to blister, then I added the carrots and gave it two more minutes.','Recipe_photos/7ce35d71-1dd9-4dbb-8b52-7229b5c1212b.jpg');
INSERT INTO "recipe_step" VALUES(308,23,14,'n with the bell peppers strips and stir-fry for about 3 minutes. Gotta keep things moving!','Recipe_photos/b763c07b-3b7c-41d5-9104-3dc9b09d866e.jpg');
INSERT INTO "recipe_step" VALUES(309,23,15,'Next up were the onions. Another 2 minutes.','Recipe_photos/fbf7db66-01b9-4b9c-9a60-8d3fcc4fe9d9.jpg');
INSERT INTO "recipe_step" VALUES(310,23,16,'The mushrooms and spring onions. Stir-fry until the mushrooms soften up.','Recipe_photos/864661ba-6654-4350-a282-e7c0eae2060b.jpg');
INSERT INTO "recipe_step" VALUES(311,23,17,'Add the chicken, give it a minute to warm up again before stirring in the bean sprouts.','Recipe_photos/370079ef-1b14-46dd-b7a9-467a78a5e20e.jpg');
INSERT INTO "recipe_step" VALUES(312,23,18,'Two full tbsp sauce is the magic amount for me. It coats the vegetables but doesn’t drown anything.','Recipe_photos/97e02ff9-5159-42c7-8c70-57d0d1859bc9.jpg');
INSERT INTO "recipe_step" VALUES(313,23,19,'Stir it in and give everything 1 more minute.','Recipe_photos/df432a29-3f50-4646-b1ec-2b3d5f00515d.jpg');
INSERT INTO "recipe_step" VALUES(314,23,20,'These vegetables were perfect, bite-tender and slightly sweet with a kick.','Recipe_photos/0e50a0f0-1c50-4085-b31e-0cd6f123580f.jpg');
INSERT INTO "recipe_step" VALUES(315,24,1,'Thinly slice the onion. I always go for quart-rings.','/media/Recipe_photos/f7aeb2ab-cf66-413f-a889-c60eea619aeb.jpg');
INSERT INTO "recipe_step" VALUES(316,24,2,'Leave the seeds in if you want it really spicy. I just go for warmth so I remove them before chopping up the pepper.','/media/Recipe_photos/56bda4a8-25be-4fa8-9eae-a54991763780.jpg');
INSERT INTO "recipe_step" VALUES(317,24,3,'Combine all ingredients in a bowl and add a pinch of salt. Adding pieces of juicy pear also works!','/media/Recipe_photos/0bcdd166-67aa-4364-8a04-49c85128dfd2.jpg');
INSERT INTO "recipe_step" VALUES(318,24,4,'Combine the honey with the ginger, ground coriander and sesame oil. See, I really used a smidgen.','/media/Recipe_photos/5260e073-614e-4205-bce9-5b49adafaaa2.jpg');
INSERT INTO "recipe_step" VALUES(319,24,5,'Add the vinegar and oil (go for a neutral-flavored light oil) to the honey.','/media/Recipe_photos/b525e296-0503-443b-851e-af08fe3fe827.jpg');
INSERT INTO "recipe_step" VALUES(320,24,6,'I love to see oil float atop. I have issues, I know.','/media/Recipe_photos/210b63b5-b369-44d3-901e-c8728615764d.jpg');
INSERT INTO "recipe_step" VALUES(321,24,7,'Mix the dressing, pour it in there and let it marinate for 15 to 30 minutes.','/media/Recipe_photos/278adfd1-8c9b-4da0-89db-301acf89ba93.jpg');
INSERT INTO "recipe_step" VALUES(322,24,8,'It’s so light, zingy and refreshing. I really do love this salad.','/media/Recipe_photos/29988cde-df5b-4db1-a249-a7f8787d2192.jpg');
INSERT INTO "recipe_step" VALUES(323,24,9,'It’s so light, zingy and refreshing. I really do love this salad.','Recipe_photos/29988cde-df5b-4db1-a249-a7f8787d2192.jpg');
INSERT INTO "recipe_step" VALUES(324,24,0,'Peel the cucumber, slice it in half, use a small spoon to remove the seeds and slice it in half-moons.','/media/Recipe_photos/70c88681-abb4-44f1-94cb-05e4a7098fa0.jpg');
INSERT INTO "recipe_step" VALUES(325,25,1,'Somehow I always find myself using the same basic ingredients such as onion, bell pepper, garlic etc. I’m pretty sure I don’t have to show you for the umpth time how to dice an onion, peel tomatoes, grate garlic and chop stuff, so I just turn this…','Recipe_photos/c42c179e-5010-4442-8e76-69d28bb30cdc.jpg');
INSERT INTO "recipe_step" VALUES(326,25,2,'….into this. There. All done.','Recipe_photos/601783c4-9e53-485b-9a02-3953b8338954.jpg');
INSERT INTO "recipe_step" VALUES(327,25,3,'Cut the chicken into tiny cubes. Remember the yellow cutting board I had? It died on me. And for the life of me, I can’t seem to find a new one. So I just wrote “Kip” (meaning chicken) on a red one, and that makes it okay to use the red one for chicken. See, I do have a logical system. I really do!  ','Recipe_photos/f6a54941-ae27-498a-9a56-4dd275bd8204.jpg');
INSERT INTO "recipe_step" VALUES(328,25,4,'Don’t you love big storyboards? I just saved your finger a 10-photo scrolling trip. Combine the oil with the sesame oil, pinch of salt, loads of pepper, chili powder (the hot version), dried thyme, ground ginger, brown sugar and garlic.   ','Recipe_photos/71d1396e-da0d-4879-9e42-d8971ee4d501.jpg');
INSERT INTO "recipe_step" VALUES(329,25,5,'Mix it with the chicken and let it marinade for 15 minutes while you move on with the recipe.','Recipe_photos/36742fe9-f519-452f-9349-af79442fb9b5.jpg');
INSERT INTO "recipe_step" VALUES(330,25,6,'This is so hard-core! I steered clear from the prewashed stuff and bought fresh spinach at the market. Okay, so I was just testing my new salad spinner, but still. Wash and dry 1/2 a pound spinach. I tend to remove the long stalks because they freak out my kid. I prefer using the leaves whole, but you can give them a chop as well.  ','Recipe_photos/ef178cb1-b3ce-4974-9db1-a35b5b4b3840.jpg');
INSERT INTO "recipe_step" VALUES(331,25,7,'No need to add oil to the pan since the marinade contained oil. Cook the chicken until it loses its rawness and add the onion and bell pepper. Cook everything for 5 more minutes. ','Recipe_photos/426a0e8b-c6c7-4a09-ae4a-d1f266a88aac.jpg');
INSERT INTO "recipe_step" VALUES(332,25,8,'Spoon the peanut butter in there, pour in the chicken broth and add the tomatoes. Bring to a boil and simmer, over low heat, for 20 to 25 minutes without the lid on.   ','Recipe_photos/81de5ab7-cc27-4ad2-a358-9f3102d6f14c.jpg');
INSERT INTO "recipe_step" VALUES(333,25,9,'After 20 minutes the sauce will have cooked down and thickened a bit. Season with salt, pepper, chili powder and brown sugar to taste. Stir in the spinach and as soon as it wilts, turn off the heat. Told you it was a simple recipe.   ','Recipe_photos/cf1bef46-1f9d-42d6-9629-1900de611d89.jpg');
INSERT INTO "recipe_step" VALUES(334,25,10,'This one covers all the bases: chicken, vegetables and peanut. How can it not be good? Serve with lots of rice, eat and be happy. Hakuna Matata!','Recipe_photos/68b0e086-b150-4552-b1a6-40862f6d07b4.jpg');
INSERT INTO "recipe_step" VALUES(335,26,1,'Peel and boil the potatoes in salted water until fork tender. I used my microwave.','Recipe_photos/d534a694-078a-461d-9c33-8d057884db55.jpg');
INSERT INTO "recipe_step" VALUES(336,26,2,'Slice the onion in half-rings, put them in a microwave-safe bowl. Grate in the garlic and season them with the crumbled 1/4 bouillon cube and pepper. Mix it up.','Recipe_photos/2b70d99a-0807-4f9d-a395-d441988c52cc.jpg');
INSERT INTO "recipe_step" VALUES(337,26,3,'You could just lightly sauté them, but I went for easy. I covered the bowl with (microwave-safe) plastic wrap and nuked them for 4 minutes at full power until they were translucent. Gramma''s Creamy Potato Bake','Recipe_photos/fc4ece7e-f213-4106-9d46-9e1b4d980e90.jpg');
INSERT INTO "recipe_step" VALUES(338,26,4,'You could go for thick slices of ham, but I prefer the very thinly sliced deli ham. Chop it up. I’ve yet to try this dish with bacon.','Recipe_photos/8f2c6313-cda0-495c-b164-b8ac28859cf3.jpg');
INSERT INTO "recipe_step" VALUES(339,26,5,'Do the same with the hardboiled eggs.','Recipe_photos/b4d7f3c1-09fa-41c2-9270-dd46b49e29ef.jpg');
INSERT INTO "recipe_step" VALUES(340,26,6,'And a good handful flat-leaf parsley.','Recipe_photos/5fafa51d-7dc1-4a05-8712-96a0ebf46e21.jpg');
INSERT INTO "recipe_step" VALUES(341,26,7,'Your onions should be all done by now.','Recipe_photos/5b611791-3f8a-4381-a6cb-ad6c036b4f18.jpg');
INSERT INTO "recipe_step" VALUES(342,26,8,'Spoon the sour cream into a bowl.','Recipe_photos/3569ede5-9be7-42ae-89e6-b2ba55140752.jpg');
INSERT INTO "recipe_step" VALUES(343,26,9,'Season it with a good pinch of salt and pepper.','Recipe_photos/0369f085-b92c-4f09-9516-14f56ec91675.jpg');
INSERT INTO "recipe_step" VALUES(344,26,10,'The coarse mustard adds pizzazz without making it mustardy. Add the ham, onion, egg and parsley. Gramma''s Creamy Potato Bake','Recipe_photos/57131f49-1854-47f8-ad47-b39d35c24c91.jpg');
INSERT INTO "recipe_step" VALUES(345,26,11,'Stir well and check the seasoning.','Recipe_photos/0f324292-0085-4bc1-9bd9-433ac4a013ef.jpg');
INSERT INTO "recipe_step" VALUES(346,26,12,'You can use a big casserole for this, but I opted for these cute little cocottes. Fake le creusets. They cost me 4 euros a piece.','Recipe_photos/737b76a5-4781-47b4-8c40-df89c5d55831.jpg');
INSERT INTO "recipe_step" VALUES(347,26,13,'Cut the potatoes in thick slices, layer them at the bottom of a lightly oiled casserole and top them with the veggie and ham mix.','Recipe_photos/7492af56-a0e0-4ae0-ba12-1949e94b77c8.jpg');
INSERT INTO "recipe_step" VALUES(348,26,14,'You can already tell how good this will taste.','Recipe_photos/c1072220-1dfe-40fd-bae8-c6ebe95c8299.jpg');
INSERT INTO "recipe_step" VALUES(349,26,15,'It tastes even better with a good handful of melting cheese sprinkled on top. But doesn’t everything?','Recipe_photos/d4ebb41c-4345-446f-804c-22724f6d5388.jpg');
INSERT INTO "recipe_step" VALUES(350,26,16,'Place the casserole in a preheated oven and bake at 400Fº (200Cº) for 20 to 25 minutes (keep an eye on it) until the cheese has melted and turned golden brown.','Recipe_photos/02344dd0-f0e5-4ef4-9095-aa7b701125df.jpg');
INSERT INTO "recipe_step" VALUES(351,27,1,'I make my Harissa mainly for warmth and flavor. If you want a really spicy Harissa leave in the seeds or opt for spicier peppers.','Recipe_photos/f04eab17-f14f-4abd-a77c-03a38b834ad0.jpg');
INSERT INTO "recipe_step" VALUES(352,27,2,'I use my mortar and pestle. It’s how I roll. Feel free to use ground spices or a spice mill.','Recipe_photos/ac3a9dec-651d-41db-aecf-534e2825b99b.jpg');
INSERT INTO "recipe_step" VALUES(353,27,3,'Combine the cumin seeds…','Recipe_photos/3fee6679-598d-4682-97d7-224336b09acf.jpg');
INSERT INTO "recipe_step" VALUES(354,27,4,'With the coriander seeds.','Recipe_photos/14111ae7-b1ec-4626-bdd2-147b44fe7836.jpg');
INSERT INTO "recipe_step" VALUES(355,27,5,'And add the caraway seeds. Cumin and caraway seeds look an awful lot alike, but taste different! Keep that in mind.','Recipe_photos/beabe15a-ff82-49ea-8ed8-09323d379868.jpg');
INSERT INTO "recipe_step" VALUES(356,27,6,'Add the coarse sea salt and start mashing it up until you have a medium ground.','Recipe_photos/2f193a78-2f8e-4b5e-aae5-a95d8f9757df.jpg');
INSERT INTO "recipe_step" VALUES(357,27,7,'Finely chop the peppers, remove the seeds if you like, and grate the garlic. Add it to your food processor along with the oil and white wine vinegar.','Recipe_photos/4240d263-8e23-4116-8beb-5f96901df2a2.jpg');
INSERT INTO "recipe_step" VALUES(358,27,8,'For a bit of freshness and pizzazz I always add lime juice.','Recipe_photos/ffa4ffb3-e3db-4ed8-a6e0-0d5f5df0b3c8.jpg');
INSERT INTO "recipe_step" VALUES(359,27,9,'Add the pimento (using a ground version this time for exact measurements), a tiny pinch of sugar and add the spices from my mortar.','Recipe_photos/dc11520e-34f5-47f1-8ca6-4cfb7207e43a.jpg');
INSERT INTO "recipe_step" VALUES(360,27,10,'Add the tomato puree and blitz until you have a wicked orange/reddish and fragrant sambal.','Recipe_photos/36fb6f32-ff79-4eb6-af45-6e274ef33df9.jpg');
INSERT INTO "recipe_step" VALUES(361,27,11,'Store it in your fridge with some extra oil poured on top. It will keep for ages.','Recipe_photos/0c83796d-7244-402c-aab3-ba7a9d407de2.jpg');
INSERT INTO "recipe_step" VALUES(362,28,1,'The pimento really cannot be substituted. It’s amazing stuff though, you get a hint of nutmeg, cloves, pepper and cinnamon all in one berry. Not a bad deal, I’d say.','Recipe_photos/32d5f583-ba3d-4c8c-b4be-f00d139202a2.jpg');
INSERT INTO "recipe_step" VALUES(363,28,2,'I used 1 tbsp pimento for my jerk seasoning.','Recipe_photos/0001df09-1a55-4087-a219-c0351a8c72ca.jpg');
INSERT INTO "recipe_step" VALUES(364,28,3,'Add 1 tbsp peppercorn and 1 tbsp chili flakes. Mash it up.','Recipe_photos/fb656fb2-4bca-4a04-8665-bb9e265f2315.jpg');
INSERT INTO "recipe_step" VALUES(365,28,4,'Add a tbsp coarse sea salt and continue to mash until you have a semi fine seasoning.','Recipe_photos/25a9319d-3b13-4ab3-af1d-e13087d1daa7.jpg');
INSERT INTO "recipe_step" VALUES(366,28,5,'Grate the ginger in there and add the thyme.','Recipe_photos/d77bcdae-9728-41ab-81b4-e8fc04bc71d7.jpg');
INSERT INTO "recipe_step" VALUES(367,28,6,'Coarsely cut up the Habanero, add the garlic and mash until it’s almost a puree.','Recipe_photos/1cc6310f-39ac-4c39-acd4-4ee3bd86975c.jpg');
INSERT INTO "recipe_step" VALUES(368,28,7,'Continue to mash until the poor sucker has been smashed to smithereens.','Recipe_photos/41e850a2-95f2-44f5-b562-632ad5fd2a84.jpg');
INSERT INTO "recipe_step" VALUES(369,28,8,'That’s your cue to add the brown sugar, soy sauce, honey and oil.','Recipe_photos/384f646b-c077-43ba-8735-57f6c82ea1cb.jpg');
INSERT INTO "recipe_step" VALUES(370,28,9,'This is one of the best marinades, ever!','Recipe_photos/6eb8af16-045a-4c7a-8f8d-41adb3cca761.jpg');
INSERT INTO "recipe_step" VALUES(371,28,10,'Finely mince the spring onions and add them to the marinade.','Recipe_photos/2a4d4c4f-6fd5-4826-85ad-d8f87e489734.jpg');
INSERT INTO "recipe_step" VALUES(372,28,11,'Add it to your chicken. Cover the bowl and let the chicken marinate (in the fridge) for a couple of hours, preferably overnight.','Recipe_photos/0431acf9-e14f-4147-800d-fea0e8217c79.jpg');
INSERT INTO "recipe_step" VALUES(373,28,12,'I put the chicken on the grill.','Recipe_photos/582c3fab-a9b2-4140-92a7-3a29b9dad51c.jpg');
INSERT INTO "recipe_step" VALUES(374,28,13,'And gave them 10 to 12 minutes on each side, until the juices that come out are clear. Internal temp should be 170F˚ (76C˚).','Recipe_photos/6e4e918c-5d47-4e2e-9a4c-006df5551670.jpg');
INSERT INTO "recipe_step" VALUES(375,28,14,'This, in my humble opinion, is one of Jamaica’s finest dishes! You haven’t truly lived until you’ve tasted Jerk chicken!','Recipe_photos/a677bccc-400f-46c9-bd87-16a66ea3b272.jpg');
INSERT INTO "recipe_step" VALUES(376,29,1,'Cut the cauliflower in small florets. Peel and rinse the potatoes.','Recipe_photos/4e54d755-0daa-4697-98b4-1fddfa077629.jpg');
INSERT INTO "recipe_step" VALUES(377,29,2,'Cut the cheese in small cubes and add it to the milk and set it aside.','Recipe_photos/78ebef5b-98b8-4a92-94ad-67c57789c337.jpg');
INSERT INTO "recipe_step" VALUES(378,29,3,'Add salt to the potatoes and boil them for 15 minutes (depending on the size). Add the cauliflower, bring them back to a boil and give them another 8 to 10 minutes.','Recipe_photos/a5cd9644-499e-4bf6-8c1b-9bfd94f16ad1.jpg');
INSERT INTO "recipe_step" VALUES(379,29,4,'As soon as you add the cauliflower you put the milk on the stove. Add the butter to it and warm it over really low heat, until the cheese melts. Do not let it boil.','Recipe_photos/6c82a097-5fdf-485e-8b7f-8ef32dec64eb.jpg');
INSERT INTO "recipe_step" VALUES(380,29,5,'As soon as the cauliflower and potatoes are done, drain the vegetables.','Recipe_photos/ba32bff4-4788-41c4-9711-2f2ddc606fe8.jpg');
INSERT INTO "recipe_step" VALUES(381,29,6,'Pour the cheese milk in there, add the grainy mustard and mash it up. Add a little more milk if you think it needs it.','Recipe_photos/a88375ba-4beb-43b9-b7ea-85490f8ac54d.jpg');
INSERT INTO "recipe_step" VALUES(382,29,7,'Simple really is what works best.','Recipe_photos/edb4e4b2-ae84-499b-a2d2-0ed72d5eb79f.jpg');
INSERT INTO "recipe_step" VALUES(383,30,1,'Carrots are and will always rank amongst my favourite vegetables. They’re dirt-cheap, too!','Recipe_photos/3538b4d0-0784-4670-9343-d3b580b8b063.jpg');
INSERT INTO "recipe_step" VALUES(384,30,2,'Wash, peel and cut them in half. Leave the smaller ones whole and transfer them to a casserole. Simple Oven-Baked Carrots','Recipe_photos/44ae5f60-b204-44c3-a6bf-19154b845c34.jpg');
INSERT INTO "recipe_step" VALUES(385,30,3,'Cut the onion in half-quarts and slice the garlic. Randomly place the onion and garlic in with the carrots. Simple Oven-Baked Carrots','Recipe_photos/28f26213-941f-44a8-9d51-9c1eb4acbc6e.jpg');
INSERT INTO "recipe_step" VALUES(386,30,4,'Toss them with the olive oil and season them with (kosher) salt and pepper. I always break out my fleur de sel for this.','Recipe_photos/52385119-2d28-4f25-aedc-857766f12b07.jpg');
INSERT INTO "recipe_step" VALUES(387,30,5,'Add the thyme and parsley and cube the butter. The butter will add a lovely flavour and richness.','Recipe_photos/31ee03de-e449-4531-8f63-c13a50a45a1f.jpg');
INSERT INTO "recipe_step" VALUES(388,30,6,'Pour the chicken broth and lemon juice into the casserole.','Recipe_photos/1ad3ae5f-3538-4faf-ad6b-5be467588091.jpg');
INSERT INTO "recipe_step" VALUES(389,30,7,'Place the butter cubes on top of the carrots. They will melt all over and do yummy things to the carrots.','Recipe_photos/a50f2068-b6fc-494a-9c0a-f5ee2500e106.jpg');
INSERT INTO "recipe_step" VALUES(390,30,8,'Tightly wrap the casserole with aluminium foil.','Recipe_photos/1422e12f-0e89-40ea-a976-3aafc6231c05.jpg');
INSERT INTO "recipe_step" VALUES(391,30,9,'  After roughly 40 minutes they are delicious. Sweet and juicy. But they need a bit of colour. A little roasting will do them good.','Recipe_photos/7e2d6ed3-8a65-4cd4-872e-42214faac37b.jpg');
INSERT INTO "recipe_step" VALUES(392,30,10,'There. That’s more like it. Rustic, gorgeous and absolutely delicious.','Recipe_photos/3b70500c-30de-41dd-9a9c-b8c9919b4837.jpg');
INSERT INTO "recipe_step" VALUES(393,31,1,'First strawberries I bought this year. Didn’t expect them to be so sweet and full of flavor already.','Recipe_photos/c38e993d-bdad-45b4-ab7b-eeb9dd82a3f3.jpg');
INSERT INTO "recipe_step" VALUES(394,31,2,'Cut each strawberry in 4 parts.','Recipe_photos/949dee5a-fa3b-4f25-b4dd-28d31e80662e.jpg');
INSERT INTO "recipe_step" VALUES(395,31,3,'Believe it or not, but I don’t have blender. I do have a blender ring for my food processor.','Recipe_photos/2565e6b9-d36b-4a1a-88ef-5b71531c439e.jpg');
INSERT INTO "recipe_step" VALUES(396,31,4,'Need I say more?','Recipe_photos/67a97d6c-9bec-403a-9741-b0d9a1893f16.jpg');
INSERT INTO "recipe_step" VALUES(397,31,5,'I used these small, 100ml ice cream packs because they contain 3 scoops each. One pack per person.','Recipe_photos/c109c2d6-dd00-47e2-affb-08ef8629f8f7.jpg');
INSERT INTO "recipe_step" VALUES(398,31,6,'Put the ice cream in your blender or food processor. See all those pieces of crust in there? Delicious.','Recipe_photos/a48cb52c-aa1a-4d8f-9454-b93fbbe84829.jpg');
INSERT INTO "recipe_step" VALUES(399,31,7,'Add the strawberries and tequila. Feel free to use less tequila, just replace whatever you leave out with more milk.','Recipe_photos/d3c16633-e080-49c8-bc3c-d153da6e0fd0.jpg');
INSERT INTO "recipe_step" VALUES(400,31,8,'Add the cold milk.','Recipe_photos/974eec50-39df-43f1-a71e-1cde7470f0b1.jpg');
INSERT INTO "recipe_step" VALUES(401,31,9,'Sprinkle the sugar in there and pour in the vanilla extract.','Recipe_photos/804354ce-0c89-4c48-a872-b4729a844a2e.jpg');
INSERT INTO "recipe_step" VALUES(402,31,10,'Blitz until the strawberries are thoroughly blended.','Recipe_photos/aa476df4-0fb7-455f-9dc9-902d460c5317.jpg');
INSERT INTO "recipe_step" VALUES(403,31,11,'This is such a perfect, refreshing ending to a summer BBQ feast.','Recipe_photos/939b478e-680c-4078-acf4-b9da66c7b110.jpg');
CREATE TABLE "recipe_vote" (
    "id" integer NOT NULL PRIMARY KEY,
